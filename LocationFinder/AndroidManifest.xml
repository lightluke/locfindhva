            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyBRbz4XfRlgB6FhtHS0E04whLj-hf6BHFY" />
        <activity
            android:name="nl.hva.proveit.locationfinder.Main"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Over"></activity>
    </application>

</manifest>